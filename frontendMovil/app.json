{
  "expo": {
    "plugins": [
      "expo-font",
      "expo-video"
    ],
    "name": "Marquesa",
    "slug": "frontendMovil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/marquesaMiniLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/images/marquesaMiniLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
