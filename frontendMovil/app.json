      "adaptiveIcon": {
        "foregroundImage": "./src/images/marquesaMiniLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dianitapadilla.frontendMovil",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./src/images/marquesaMiniLogo.png"
    },
    "extra": {
      "eas": {
        "projectId": "bf95e9b0-acc9-4965-866b-e426f2aa0145"
      }
    }
  }
}